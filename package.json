    "frontend": "nx serve frontend",
    "backend": "nx serve backend",
    "scheduler": "nx serve task-runner",
    "build:fe": "nx build frontend --prod --skip-nx-cache",
    "build:be": "nx build backend --prod --skip-nx-cache",
    "build:se": "nx build task-runner --prod --skip-nx-cache",
    "build:all": "yarn build:fe && yarn build:be",
    "prod:fe": "next start ./dist/apps/frontend/",
    "prod:be": "node ./dist/apps/backend/main.js",
    "prod:se": "node ./dist/packages/task-runner/main.js",
    "prisma": "prisma",
    "generate": "prisma generate --schema=.\\packages\\shared-models\\src\\prisma\\schema.prisma",
